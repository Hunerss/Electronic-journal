<Window x:Class="Electronic_journal.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Electronic_journal"
        mc:Ignorable="d"
        Icon="/Images/icon.ico"
        WindowStartupLocation="CenterScreen"
        Title="Electronic Gradebook" Height="500" Width="820">
    <Window.Background>
        <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
            <GradientStop Color="#90CAF9" Offset="0.0" />
            <GradientStop Color="#2196F3" Offset="1.0" />
        </LinearGradientBrush>
    </Window.Background>
    <Grid>
        <Frame x:Name="frame" NavigationUIVisibility="Hidden" />
    </Grid>
</Window>
